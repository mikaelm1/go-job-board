{{>header}}

<div class="container">
    <div class="row">
        <div class="col-md-6 col-md-offset-3">
            <form action="/education/login" method="post">
                <div class="form-group">
                    <label for="exampleInputEmail1">School Name</label>
                    <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" name="email" placeholder="Enter email">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">Major</label>
                    <input type="password" class="form-control" name="password" id="exampleInputPassword1" placeholder="Password">
                </div>
                <div class="form-group">
                    <label for="exampleSelect1">Year Started</label>
                    <select class="form-control" id="exampleSelect1" name="yearEnded">
                    <option>2012</option>
                    <option>2013</option>
                    <option>2014</option>
                    <option>2015</option>
                    <option>2016</option>
                    <option>2017</option>
                    </select>
                </div>
                <div class="form-group">
                    <label for="exampleSelect1">Year Ended</label>
                    <select class="form-control" id="exampleSelect1" name="yearEnded">
                    <option>2012</option>
                    <option>2013</option>
                    <option>2014</option>
                    <option>2015</option>
                    <option>2016</option>
                    <option>2017</option>
                    <option>2018</option>
                    <option>2019</option>
                    <option>2020</option>
                    </select>
                </div>
                <button type="submit" class="btn btn-primary">Add</button>
            </form>
        </div>
    </div>
</div>